{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES6",
    "allowSyntheticDefaultImports": false,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "ChromeApi/*": ["./client/src/scripts/chrome/*"],
      "GlobalActionCreators/*": ["./client/src/actionCreators/*"],
      "GlobalActionTypes/*": ["./client/src/actionTypes/*"],
      "GlobalApis/*": ["./client/src/apis/*"],
      "GlobalComponents/*": ["./client/src/components/*"],
      "GlobalConstants/*": ["./client/src/constants/*"],
      "GlobalContainers/*": ["./client/src/containers/*"],
      "GlobalIcons/*": ["./client/src/icons/*"],
      "GlobalReducers/*": ["./client/src/reducers/*"],
      "GlobalScripts/*": ["./client/src/scripts/*"],
      "GlobalStyles/*": ["./client/src/styles/*"],
      "GlobalUtils/*": ["./client/src/utils/*"],
      "SrcPath/*": ["./client/src/*"]
    }
  },
  "exclude": ["build", "extension", "firebase-build", "node_modules"]
}
