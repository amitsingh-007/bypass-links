{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "better-npm-run build",
    "dev": "npm run pre:dev && better-npm-run dev",
    "dev:debug": "better-npm-run dev:debug",
    "pre:dev": "cd .. && vercel env pull web/.env.local"
  },
  "betterScripts": {
    "dev": {
      "command": "next dev",
      "env": {
        "HOST_NAME": "http://localhost:3000"
      }
    },
    "dev:debug": {
      "command": "npm run dev",
      "env": {
        "NODE_OPTIONS": "--inspect"
      }
    },
    "build": {
      "command": "next build",
      "env": {
        "HOST_NAME": "https://bypass-links.vercel.app"
      }
    }
  },
  "dependencies": {
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@mui/lab": "5.0.0-alpha.103",
    "@mui/material": "5.10.9",
    "@octokit/rest": "19.0.5",
    "express-bearer-token": "2.4.0",
    "firebase-admin": "11.1.0",
    "helmet": "6.0.0",
    "next": "12.3.1",
    "nocache": "3.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.6.0",
    "speakeasy": "2.0.0"
  },
  "devDependencies": {
    "@svgr/webpack": "6.5.0",
    "@types/compression": "1.7.2",
    "@types/express": "4.17.14",
    "@types/speakeasy": "2.0.7",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "better-npm-run": "0.1.1",
    "eslint": "8.27.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-webpack-plugin": "3.2.0",
    "fork-ts-checker-webpack-plugin": "7.2.13",
    "next-pwa": "5.6.0",
    "prettier": "2.7.1",
    "sass": "1.55.0",
    "typescript": "4.8.4",
    "vercel": "28.4.10"
  }
}
