{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "better-npm-run build",
    "dev": "npm run pre:dev && better-npm-run dev",
    "lint": "next lint",
    "pre:dev": "cd .. && vercel env pull web/.env.local",
    "start": "next start"
  },
  "betterScripts": {
    "dev": {
      "command": "next dev",
      "env": {
        "NODE_OPTIONS": "--inspect",
        "HOST_NAME": "http://localhost:3000"
      }
    },
    "build": {
      "command": "next build",
      "env": {
        "HOST_NAME": "https://bypass-links.vercel.app"
      }
    }
  },
  "dependencies": {
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@mui/material": "5.2.3",
    "@mui/lab": "5.0.0-alpha.59",
    "@octokit/rest": "18.12.0",
    "cors": "2.8.5",
    "express-bearer-token": "2.4.0",
    "firebase-admin": "10.0.1",
    "helmet": "4.6.0",
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "4.3.1",
    "speakeasy": "2.0.0"
  },
  "devDependencies": {
    "@svgr/webpack": "6.1.2",
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/speakeasy": "2.0.6",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "better-npm-run": "0.1.1",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "fork-ts-checker-webpack-plugin": "6.5.0",
    "next-pwa": "5.4.4",
    "prettier": "2.5.1",
    "sass": "1.45.0",
    "vercel": "23.1.2"
  }
}
