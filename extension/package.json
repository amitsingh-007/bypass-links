{
  "name": "extension",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "better-npm-run dev",
    "build": "better-npm-run build"
  },
  "betterScripts": {
    "build": {
      "command": "webpack --config webpack/firebase.config.js && webpack --config webpack/extension.config.js",
      "env": {
        "NODE_ENV": "production",
        "HOST_NAME": "https://bypass-links.vercel.app"
      }
    },
    "dev": {
      "command": "webpack --progress --config webpack/firebase.config.js && webpack -w --progress --config webpack/extension.config.js",
      "env": {
        "NODE_ENV": "development",
        "HOST_NAME": "http://localhost:3000"
      }
    }
  },
  "dependencies": {
    "@emotion/react": "11.4.0",
    "@emotion/styled": "11.3.0",
    "@material-ui/core": "5.0.0-beta.0",
    "@material-ui/icons": "5.0.0-beta.0",
    "@material-ui/lab": "5.0.0-alpha.40",
    "@material-ui/styles": "5.0.0-beta.0",
    "browser-image-compression": "1.0.14",
    "core-js": "3.15.2",
    "dayjs": "1.10.5",
    "firebase": "8.7.0",
    "md5": "2.3.0",
    "qrcode": "1.4.4",
    "react": "17.0.2",
    "react-beautiful-dnd": "13.1.0",
    "react-dom": "17.0.2",
    "react-easy-crop": "3.5.2",
    "react-intersection-observer": "8.32.0",
    "react-redux": "7.2.4",
    "react-ripples": "2.2.1",
    "react-router-dom": "5.2.0",
    "redux": "4.1.0",
    "throttle-debounce": "3.0.1",
    "utility-types": "3.10.0"
  },
  "devDependencies": {
    "@svgr/webpack": "5.5.0",
    "@types/chrome": "0.0.145",
    "@types/material-ui": "0.21.9",
    "@types/md5": "2.3.1",
    "@types/qrcode": "1.4.1",
    "@types/react-beautiful-dnd": "13.1.1",
    "@types/react-dom": "17.0.9",
    "@types/react-router-dom": "5.1.7",
    "@types/throttle-debounce": "2.1.0",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "better-npm-run": "0.1.1",
    "css-loader": "5.2.6",
    "css-minimizer-webpack-plugin": "3.0.2",
    "eslint": "7.30.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "filemanager-webpack-plugin": "6.1.4",
    "fork-ts-checker-webpack-plugin": "6.2.12",
    "html-webpack-plugin": "5.3.2",
    "mini-css-extract-plugin": "2.0.0",
    "node-sass": "6.0.1",
    "sass-loader": "12.1.0",
    "ts-loader": "9.2.3",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "typescript": "4.3.5",
    "webpack": "5.42.0",
    "webpack-cli": "4.7.2",
    "webpack-merge": "5.8.0",
    "webpack-watch-external-files-plugin": "1.0.0"
  }
}
