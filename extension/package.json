{
  "name": "extension",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "better-npm-run build && npm run post:build",
    "dev": "better-npm-run dev",
    "post:build": "node scripts/manifest-file && node scripts/archive"
  },
  "betterScripts": {
    "build": {
      "command": "webpack --config webpack/firebase.config.ts && webpack --config webpack/extension.config.ts",
      "env": {
        "NODE_ENV": "production",
        "HOST_NAME": "https://bypass-links.vercel.app"
      }
    },
    "dev": {
      "command": "webpack --progress --config webpack/firebase.config.ts && webpack serve --progress --config webpack/extension.config.ts",
      "env": {
        "NODE_ENV": "development",
        "HOST_NAME": "http://localhost:3000"
      }
    }
  },
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@hello-pangea/dnd": "16.2.0",
