{
  "version": "2.1.0",
  "manifest_version": 2,
  "short_name": "Bypass Links",
  "name": "Bypass Links",
  "author": "Amit Singh",
  "description": "Bypass links from various sites to required links.",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Bypass Links"
  },
  "icons": {
    "128": "bypass_128.png"
  },
  "permissions": ["storage", "tabs", "history", "<all_urls>"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "commands": {
    "toggle_bypass_links_extension": {
      "suggested_key": {
        "default": "Ctrl+M",
        "mac": "Command+M"
      },
      "description": "Toggle the extension"
    }
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
