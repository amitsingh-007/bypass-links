{
  "manifest_version": 2,
  "short_name": "Bypass Links",
  "name": "Bypass Links",
  "author": "Amit Singh",
  "version": "1.1.0",
  "description": "Bypass links from various sites to required links.",
  "icons": {
    "128": "bypass_128.png"
  },
  "browser_action": {},
  "permissions": ["tabs", "<all_urls>"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
