  },
  "betterScripts": {
    "dev": {
      "command": "webpack --config webpack.firebase.config.js && webpack --watch",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "release": {
      "command": "webpack --config webpack.firebase.config.js && webpack",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "webpack serve",
      "env": {
        "NODE_ENV": "development",
        "DEV_SERVER": true
      }
    }
