{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es6",
    "module": "es6",
    "types": ["cypress"],
    "allowJs": true,
    "moduleResolution": "node",
    "lib": ["es2015", "es2016", "dom"],
    "skipLibCheck": true,
    "pretty": true,
    "outDir": "./dist"
  },
  "include": ["./cypress/**/*.*"],
  "exclude": ["node_modules", "./dist"]
}
