{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "npm",
      "script": "build",
      "group": "build",
      "problemMatcher": ["$ts-checker5-webpack", "$ts-checker5-eslint-webpack"]
    },
    {
      "type": "npm",
      "script": "lint",
      "group": "build",
      "problemMatcher": ["$eslint-stylish"]
    },
    {
      "type": "npm",
      "script": "watch",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "isBackground": true,
      "problemMatcher": [
        "$ts-checker5-webpack-watch",
        "$ts-checker5-eslint-webpack-watch"
      ]
    }
  ]
}
