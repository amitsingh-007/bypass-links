{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Netlify Debugging",
      "type": "pwa-node",
      "request": "launch",
      "runtimeArgs": ["run-script", "server:serve"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**", "**/node_modules/**/*.js"],
      "outFiles": ["${workspaceFolder}/server/**/*.js"],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ]
    }
  ]
}
