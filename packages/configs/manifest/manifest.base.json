{
  "version": "22.4.0",
  "manifest_version": 3,
  "short_name": "Bypass Links",
  "name": "Bypass Links",
  "author": "Amit Singh",
  "description": "Bypass links from various sites to required links, with added functionality of Bookmarks Manager, Tagging Panel.",
  "action": {
    "default_icon": "assets/bypass_link_on_32.png",
    "default_popup": "index.html",
    "default_title": "Bypass Links"
  },
  "icons": {
    "32": "assets/bypass_link_on_32.png",
    "48": "assets/bypass_link_on_48.png"
  },
  "oauth2": {
    "scopes": ["https://www.googleapis.com/auth/userinfo.email"]
  },
  "permissions": [
    "storage",
    "tabs",
    "history",
    "identity",
    "scripting",
    "webNavigation"
  ],
  "host_permissions": ["<all_urls>"],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
  }
}
